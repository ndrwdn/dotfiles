#git fetch zsh-users_zsh-autosuggestions
#git subtree pull --prefix .zsh/zsh-autosuggestions zsh-users_zsh-autosuggestions master --squash
[push]
  default = simple
[include]
  path = .gitconfig.local
[diff "blackbox"]
  textconv = gpg --use-agent -q --batch --decrypt
